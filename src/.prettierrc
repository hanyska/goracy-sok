{
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "parser": "typescript",
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "overrides": [{
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "html",
        "bracketSameLine": true
      }
    }]
  }
  